{
    "cli": {
        "version": ">= 5.0.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "ios": {
                "simulator": false
            },
            "env": {
                "EXPO_PUBLIC_API_URL": "http://192.168.0.101:4000/graphql"
            }
        },
        "development-simulator": {
            "developmentClient": true,
            "distribution": "internal",
            "ios": {
                "simulator": true
            },
            "env": {
                "EXPO_PUBLIC_API_URL": "http://localhost:4000/graphql"
            }
        },
        "preview": {
            "distribution": "internal",
            "env": {
                "EXPO_PUBLIC_API_URL": "https://api.onlyou.life/graphql"
            }
        },
        "production": {
            "autoIncrement": true,
            "env": {
                "EXPO_PUBLIC_API_URL": "https://api.onlyou.life/graphql"
            }
        }
    }
}
